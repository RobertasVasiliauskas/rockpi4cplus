#include "rk3399-u-boot.dtsi"
#include "rk3399-sdram-lpddr4-100.dtsi"

/ {
	chosen {
		u-boot,spl-boot-order = "same-as-spl", &spi_flash, &sdhci, &sdmmc;
	};
};

&spi1 {
	spi_flash: flash@0 {
		u-boot,dm-pre-reloc;
	};
};
